<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <RootNamespace>ValorantCardandTitleChanger</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <StartupObject>ValorantCardandTitleChanger.App</StartupObject>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
    <FileVersion>0.1.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Fonts\VarelaRound-Regular.ttf" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ValAPI.Net" Version="1.3.1" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Assets\Fonts\VarelaRound-Regular.ttf" />
  </ItemGroup>

</Project>
